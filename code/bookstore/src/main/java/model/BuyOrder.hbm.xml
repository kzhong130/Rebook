<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.BuyOrder" table="buyOrder">
		<id name="buyID" type="int">
			<column name="buyID" />
			<generator class="assigned"/>
		</id>
		<property name="bookRecordID" type="int">
			<column name="bookRecordID"/>
		</property>
		<property name="buyerID" type="int">
			<column name="buyerID"/>
		</property>
		<property name="buyAddress" type="java.lang.String">
			<column name="buyAddress"/>
		</property>
		<property name="buyPhone" type="java.lang.String">
			<column name="buyPhone"/>
		</property>
		<property name="receiver" type="java.lang.String">
			<column name="receiver"/>
		</property>
		<property name="arrivalTime" type="java.sql.Timestamp">
			<column name="arrivalTime"/>
		</property>
		<property name="status" type="java.lang.String">
			<column name="status"/>
		</property>
		<property name="ownerID" type="int">
			<column name="ownerID"/>
		</property>
	</class>
</hibernate-mapping>